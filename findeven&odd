#include<stdio.h>
main()
{
    int a,r,i,even=0,odd=0;
    for(i=1;i<=10;i++)
    {
        printf("Enter the number= ");
        scanf("%d",&a);
        r=a%2;
        if(r==0)
        {
            even++;
        }
        else
        {
            odd++;
        }
    }
        printf("\nTotal even number= %d",even);
        printf("\nTotal odd number= %d",odd);
}
