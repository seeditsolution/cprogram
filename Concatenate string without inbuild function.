#include<stdio.h>
#include<conio.h>
#include<string.h>
int main()
{
	char str1[50],str2[50];
	int i,j;
	printf("Enter String 1\n"); 
	gets(str1);
	
	printf("Enter String 2\n");
	gets(str2);
	
	for(i=strlen(str1),j=0;str2[j]!='\0';i++,j++)
	{
		if(i=strlen(str1))
		{
			printf(" ");
			continue;
		}
		else
		   str1[i]=str2[j];
	}
	
	str1[i]='\0';
	printf("The Concatenate String is %s",str1);
	//getch();
	return 0;
}
