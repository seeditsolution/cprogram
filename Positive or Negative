#include <stdio.h>

int main()
{
  int number;
  
  printf("Enter any number:\n");
  scanf("%d",&number);
 
  if (number >= 0)
  {
    if (number > 0)
      printf("%d is Positive Number", number);
    else
      printf("You have entered Value zero.");
  }
  else
    printf("%d is Negative Number", number);
 
 return 0;
}
