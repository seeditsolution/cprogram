#include<stdio.h>
int main()
{
	int i,n,a[10];
	printf("Enter the size of an array\n");
	scanf("%d",&n);
	printf("Enter the elements in an array\n");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("even no in an array\n");
	for(i=0;i<n;i++)
	{
		if(a[i]%2==0)
		{
			printf("%d\n",a[i]);
		}
	}
	printf("odd no in an array\n");
	for(i=0;i<n;i++)
	{
		if((a[i]%2)!=0)
		{
			printf("%d\n",a[i]);
		}
	}
	return 0;
}
