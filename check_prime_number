#include <stdio.h>

int main() { 
   int loop, number;
   int prime = 1;
   
   printf("Enter a number");
   scanf("%d", &number);
   
   if(number <= 1){
       prime = 0;
   }
   else{
        for(loop = 2; loop < number; loop++) {
          if((number % loop) == 0) {
             prime = 0;
          }
        }
   }

   if (prime == 1)
      printf("%d is prime number.", number);
   else
      printf("%d is not a prime number.", number);
   return 0;
}
